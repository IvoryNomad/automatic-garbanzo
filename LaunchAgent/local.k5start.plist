<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>$REALM_REV.k5start</string>
    <key>ProgramArguments</key>
    <array>
        <string>$HOME/.local/bin/k5start-wrapper.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>StandardErrorPath</key>
    <string>/tmp/k5start.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/k5start.out</string>
</dict>
</plist>
